{
  "name": "@radiaated/yts-scraper",
  "version": "1.0.0",
  "description": "Scrape YTS movie data by IMDB ID",
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": {
        "types": "./dist/index.d.mts",
        "import": "./dist/index.mjs"
      },
      "require": {
        "types": "./dist/index.d.ts",
        "require": "./dist/index.js"
      }
    }
  },
  "scripts": {
    "build": "tsup"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/radiaated/yts-scraper"
  },
  "keywords": [
    "yts",
    "scraper",
    "movies"
  ],
  "author": "radiaated",
  "license": "MIT",
  "dependencies": {
    "cheerio": "^1.1.2"
  },
  "devDependencies": {
    "tsup": "^8.5.0",
    "typescript": "^5.9.3"
  }
}
